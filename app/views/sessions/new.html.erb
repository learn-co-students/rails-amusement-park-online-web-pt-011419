<%= form_tag("/signin", method: "post") do %>
  <%= label_tag :name, 'Your name' %>
  <%= collection_select(:user, :name, User.all, :name, :name, {include_blank: "Please select your username from this dropdown"}) %><br>
  <%= label_tag :password, 'Your password:' %><br>
  <%= password_field_tag :password %><br>
  <%= submit_tag 'Sign In' %>
<% end %>
